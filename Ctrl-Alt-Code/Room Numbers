Scanner ob=new Scanner(System.in);
        int count=0;
        int n=ob.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=ob.nextInt();
        }
        Arrays.sort(a);
        int t;
        for(int i=0;i<n-1;i++)
        {
            if(a[i]!=a[i+1])
                count++;
        }
        count++;
        System.out.println(count);
